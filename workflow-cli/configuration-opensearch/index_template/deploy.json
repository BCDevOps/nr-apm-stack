{
  "index_patterns": [
    "nrm-deploy-*"
  ],
  "template": {
    "aliases": {
      "nrm-deploy": {}
    },
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 1
    },
    "mappings": {
      "dynamic": "strict",
      "properties": {
        "labels": {
          "type": "object",
          "properties": {
            "deploy": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "build": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        }
      }
    }
  },
  "priority": 300,
  "composed_of": [
    "ecs_agent_8.4",
    "ecs_base_8.4",
    "ecs_ecs_8.4",
    "ecs_event_8.4",
    "ecs_log_8.4",
    "ecs_host_8.4",
    "ecs_service_8.4",
    "ecs_user_8.4",
    "ecs_nrm_base_1.0",
    "ecs_nrm_kinesis_1.0"
  ],
  "version": 3,
  "_meta": {
    "description": "NRM Deploy indices"
  }
}
