{
  "index_patterns": [
    "nrm-audit-broker-*"
  ],
  "template": {
    "aliases": {
      "nrm-audit": {},
      "nrm-audit-broker": {}
    },
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 1
    },
    "mappings": {
      "dynamic": "strict",
      "properties": {
        "auth": {
          "properties": {
            "client_id": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "client_token": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "exp": {
              "type": "long"
            },
            "exp_timestamp": {
              "type": "date"
            },
            "iat": {
              "type": "long"
            },
            "iat_timestamp": {
              "type": "date"
            },
            "nbf": {
              "type": "long"
            },
            "nbf_timestamp": {
              "type": "date"
            },
            "jti": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "sub": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        },
        "error": {
          "properties": {
            "structured_data": {
              "type": "flat_object"
            }
          }
        },
        "graph": {
          "properties": {
            "edge": {
              "properties": {
                "source": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "target": {
                  "ignore_above": 1024,
                  "type": "keyword"
                }
              }
            },
            "id": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "name": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "prop": {
              "type": "flat_object"
            },
            "set": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "vertex": {
              "properties": {
                "collection": {
                  "ignore_above": 1024,
                  "type": "keyword"
                }
              }
            }
          }
        },
        "labels": {
          "type": "object",
          "properties": {
            "action_id": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        },
        "package": {
          "properties": {
            "build_guid": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "build_number": {
              "type": "long"
            },
            "source": {
              "type": "object",
              "properties": {
                "action": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "intention": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "trace": {
                  "properties": {
                    "id": {
                      "ignore_above": 1024,
                      "type": "keyword"
                    }
                  }
                },
                "transaction": {
                  "properties": {
                    "id": {
                      "ignore_above": 1024,
                      "type": "keyword"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "priority": 300,
  "composed_of": [
    "ecs_agent_8.11",
    "ecs_base_8.11",
    "ecs_cloud_8.11",
    "ecs_ecs_8.11",
    "ecs_error_8.11",
    "ecs_event_8.11",
    "ecs_host_8.11",
    "ecs_package_8.11",
    "ecs_service_8.11",
    "ecs_source_8.11",
    "ecs_tracing_8.11",
    "ecs_url_8.11",
    "ecs_user_agent_8.11",
    "ecs_user_8.11",
    "ecs_nrm_base_1.0",
    "ecs_nrm_kinesis_1.0"
  ],
  "version": 3,
  "_meta": {
    "description": "NRM audit broker indices"
  }
}
