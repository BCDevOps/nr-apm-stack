                  "type": "long"
                },
                "VmPTE": {
                  "type": "long"
                },
                "VmSwap": {
                  "type": "long"
                },
                "VmPeak": {
                  "type": "long"
                },
                "VmSize": {
                  "type": "long"
                },
                "VmLck": {
                  "type": "long"
                },
                "VmHWM": {
                  "type": "long"
                },
                "VmRSS": {
                  "type": "long"
                },
                "VmData": {
                  "type": "long"
                }
              }
            }
          }
        }
      }
    }
  }
