{
  "name": "workflow-cli",
  "description": "OpenSearch Workflow CLI",
  "version": "1.0.0",
  "author": "Matthew Bystedt",
  "bin": {
    "workflow-cli": "./bin/run"
  },
  "bundledDependencies": true,
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/client-elasticsearch-service": "^3.509.0",
    "@aws-sdk/client-sqs": "^3.509.0",
    "@aws-sdk/credential-provider-node": "^3.509.0",
    "@oclif/core": "^3.19.1",
    "@oclif/plugin-help": "^6.0.12",
    "@oclif/plugin-plugins": "^4.2.1",
    "@smithy/node-http-handler": "^2.3.1",
    "@smithy/protocol-http": "^3.1.1",
    "@smithy/signature-v4": "^2.1.1",
    "@types/ejs": "^3.1.5",
    "axios": "^1.6.7",
    "ejs": "^3.1.9",
    "hpagent": "^1.2.0",
    "ora": "^5.4.1",
    "tar-stream": "^3.1.7"
  },
  "devDependencies": {
    "@oclif/test": "^3.1.14",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.16",
    "@types/tar-stream": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chai": "^5.0.3",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jest": "^27.6.3",
    "globby": "^11.1.0",
    "mocha": "^10.2.0",
    "npm-pack-zip": "^1.3.0",
    "oclif": "^4.4.7",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.4.0",
    "typescript": "^5.3.3"
  },
  "dirname": "oex",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/configuration-opensearch",
    "/configuration-reindex",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "workflow-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc.json",
    "check-lint": "eslint . --ext .ts --config .eslintrc.json",
    "pack": "npm-pack-zip",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "manifest": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
