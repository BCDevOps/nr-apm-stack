{
  "name": "workflow-cli",
  "description": "OpenSearch Workflow CLI",
  "version": "1.0.0",
  "author": "Matthew Bystedt",
  "bin": {
    "workflow-cli": "./bin/run"
  },
  "bundledDependencies": true,
  "dependencies": {
    "@aws-crypto/sha256-js": "^4.0.0",
    "@aws-sdk/client-elasticsearch-service": "^3.341.0",
    "@aws-sdk/client-sqs": "^3.341.0",
    "@aws-sdk/credential-provider-node": "^3.341.0",
    "@aws-sdk/node-http-handler": "^3.341.0",
    "@aws-sdk/protocol-http": "^3.341.0",
    "@aws-sdk/signature-v4": "^3.329.0",
    "@oclif/core": "^2.8.5",
    "@oclif/plugin-help": "^5.2.9",
    "@oclif/plugin-plugins": "^3.1.2",
    "@types/ejs": "^3.1.2",
    "ejs": "^3.1.9",
    "hpagent": "^1.2.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.21",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.34",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "chai": "^4.3.7",
    "eslint": "^8.41.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jest": "^27.2.1",
    "globby": "^11.1.0",
    "mocha": "^10.2.0",
    "npm-pack-zip": "^1.3.0",
    "oclif": "^3.9.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.9.5"
  },
  "dirname": "oex",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/configuration-keycloak",
    "/configuration-opensearch",
    "/configuration-reindex",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "workflow-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc.json",
    "check-lint": "eslint . --ext .ts --config .eslintrc.json",
    "pack": "npm-pack-zip",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "manifest": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "bundleDependencies": [
    "@aws-crypto/sha256-js",
    "@aws-sdk/client-elasticsearch-service",
    "@aws-sdk/client-sqs",
    "@aws-sdk/credential-provider-node",
    "@aws-sdk/node-http-handler",
    "@aws-sdk/protocol-http",
    "@aws-sdk/signature-v4",
    "@oclif/core",
    "@oclif/plugin-help",
    "@oclif/plugin-plugins",
    "@types/ejs",
    "ejs",
    "hpagent",
    "ora"
  ]
}
