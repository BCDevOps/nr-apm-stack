{
  "name": "workflow-cli",
  "description": "OpenSearch Workflow CLI",
  "version": "1.0.0",
  "author": "Matthew Bystedt",
  "bin": {
    "workflow-cli": "./bin/run"
  },
  "bundledDependencies": true,
  "dependencies": {
    "@aws-crypto/sha256-js": "^2.0.1",
    "@aws-sdk/client-elasticsearch-service": "^3.95.0",
    "@aws-sdk/credential-provider-node": "^3.95.0",
    "@aws-sdk/node-http-handler": "^3.94.0",
    "@aws-sdk/protocol-http": "^3.78.0",
    "@aws-sdk/signature-v4": "^3.78.0",
    "@keycloak/keycloak-admin-client": "^18.0.0",
    "@oclif/core": "^1.9.0",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-plugins": "^2.1.0",
    "@types/ejs": "^3.1.1",
    "ejs": "^3.1.8",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@oclif/test": "^2.1.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.36",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "chai": "^4.3.6",
    "eslint": "^8.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jest": "^26.2.2",
    "globby": "^11.1.0",
    "mocha": "^10.0.0",
    "oclif": "^3.0.1",
    "npm-pack-zip": "^1.3.0",
    "shx": "^0.3.4",
    "ts-node": "^10.8.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "dirname": "oex",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/configuration-keycloak",
    "/configuration-opensearch",
    "/configuration-reindex",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "workflow-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc.json",
    "check-lint": "eslint . --ext .ts --config .eslintrc.json",
    "pack": "npm-pack-zip",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "manifest": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
